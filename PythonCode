    def __init__(self, nome, dataAdmissao, salario):
        self.nome = nome
        self.dataAdmissao = dataAdmissao
        self.salario = salario
    def setNome(self, nome):
        self.nome = nome
    def setData(self, dataAdmissao):
        self.dataAdmissao = dataAdmissao
    def setSalario(self, salario):
        self.salario = salario
    def get(self):
        return self.nome, self.dataAdmissao, self.salario
       
